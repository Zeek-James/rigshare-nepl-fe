{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "apps/admin/build"
      }
    }
  ],
  "buildCommand": "CI=false npm run build:admin",
  "outputDirectory": "apps/admin/build",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}